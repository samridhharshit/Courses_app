      <!-- Footer -->
      <div class="footer">
        <div class="row footer">
          <div class="col-sm-12">
            <div class="row collections">
              <div class="col-sm-12 col-lg-4 info">
                <h1 class="footer-text">Information</h1>
                <a class="footer-text">Contact Us</a>
                <a class="footer-text">Legal</a>
                <a class="footer-text">Rider Accessibility Program</a>
              </div>
              <div class="col-sm-12 col-lg-4 info">
                <h1 class="footer-text">Our Offers</h1>
                <a class="footer-text">(856)-568-789</a>
                <a class="footer-text">Press</a>
                <a class="footer-text">Covers</a>
              </div>
              <div class="col-sm-12 col-lg-4 info">
                <h1 class="footer-text">Media Support</h1>
                <a class="footer-text">Contact Us</a>
                <a class="footer-text">Legal</a>
                <a class="footer-text">Rider Accessibility Program</a>
