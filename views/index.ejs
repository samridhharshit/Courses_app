<!DOCTYPE html>
<html>
  <head>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="css/style.css" />
    <title>Ejs template</title>
  </head>
  <body>
    <div class="container-fluid">
      <!-- Navigation -->
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="#">Navbar</a>
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="#"
                >Navbar <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="#"
                >Login <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="#"
                >Register <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="#"
                >Wishlist <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="#"
                >Checkout <span class="sr-only">(current)</span></a
              >
            </li>
          </ul>
          <form class="form-inline my-2 my-lg-0">
            <input
              class="form-control mr-sm-2"
              type="search"
              placeholder="Search"
              aria-label="Search"
            />
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">
              Search
            </button>
          </form>
        </div>
      </nav>

      <!-- logo section -->
      <div class="logo-section logo-section-main-container">
        <div class="row logo-section">
          <div class="col-lg-8 col-md-12 logo-section">
            <img class="ebay" src="ebay.jpg" alt="ebay" />
          </div>
          <div class="col-lg-4 col-md-12 logo-section">
            <div class="row logo-section">
              <div class="col"></div>
              <div class="col-lg-6 col-sm-12" style="margin-right: 40px;">
                <div class="card">
                  <li class="list-group-item">courses in cart count: [0]</li>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Third Navigation -->
      <nav class="navbar navbar-expand-lg navbar-light bg-light third-nav">
        <a class="navbar-brand" href="#">Home</a>
        <a class="navbar-brand" href="#">About</a>
        <a class="navbar-brand" href="#">Help</a>
      </nav>

      <br /><br />

      <!-- carasoul -->
      <div
        id="carouselExampleControls"
        class="carousel slide"
        data-ride="carousel"
      >
        <div class="carousel-inner">
          <div class="carousel-item">
            <img src="course1.png" class="carosal" alt="course1" />
          </div>
          <div class="carousel-item">
            <img src="course2.jpeg" class="carosal" alt="course2" />
          </div>
          <div class="carousel-item active">
            <img src="course3.png" class="carosal" alt="course3" />
          </div>
        </div>
        <a
          class="carousel-control-prev"
          href="#carouselExampleControls"
          role="button"
          data-slide="prev"
        >
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a
          class="carousel-control-next"
          href="#carouselExampleControls"
          role="button"
          data-slide="next"
        >
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>

      <br /><br />

      <!-- Best Courses -->
      <div class="courses">
        <div class="row">
          <div class="col">
            <h1>Best Courses</h1>
          </div>
        </div>
      </div>

      <!-- ############################################################################## -->
      <!-- Best Courses-content -->

      <div class="best-courses">
        <div class="row">
          <div class="col-sm-6">
            <div class="card courses">
              <img
                src="<%= obj.rated[0].image_link %>"
                class="card-img-top"
                alt="best-course2"
              />
              <div class="card-body">
                <h5 class="card-title"><%= obj.rated[0].course_name %></h5>
                <p class="card-text">
                  <%= obj.rated[0].description %>
                </p>
                <!-- price -->
                <div class="price">
                  <p class="price"> ₹<%= obj.rated[0].price %></p>
                  <a  class="btn btn-primary direct">Go somewhere</a>
                </div>
              </div>
            </div>
          </div>
          <div class="col-sm-6">
            <div class="card courses">
              <img
                src="<%= obj.rated[1].image_link %>"
                class="card-img-top"
                alt="best-course1"
